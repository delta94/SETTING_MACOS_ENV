{
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.matchBrackets": "never",
  "editor.wordBasedSuggestions": true,
  "editor.renderLineHighlight": "gutter",
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.tabCompletion": "on",
  "editor.fontSize": 16,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 80,
  "prettier.bracketSpacing": true,
  "prettier.semi": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSaveTimeout": 300,
  "workbench.iconTheme": "vscode-icons",
  "window.zoomLevel": -1,
  "files.autoSave": "off",
  "files.associations": {
    "*.json": "jsonc"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[handlebars]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "terminal.integrated.rendererType": "dom",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "typescript.format.enable": false,
  "typescript.suggestionActions.enabled": false,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.tsc.autoDetect": "off",
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  "breadcrumbs.enabled": true,
  "flow.enabled": false,
  "sync.gist": "ef480108830c863014df47e35067f78c",
  "sync.autoUpload": true,
  "sync.syncExtensions": true,
  "git.autofetch": true,
  "todohighlight.isEnable": false,
  "cSpell.enabled": false,
  "editor.lineNumbers": "on",
  "sync.forceUpload": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "code-runner.runInTerminal": true,
  "csstree.enable": true,
  "css.lint.float": "error",
  "css.lint.boxModel": "error",
  "css.lint.important": "error",
  "css.lint.emptyRules": "error",
  "css.lint.hexColorLength": "error",
  "css.lint.idSelector": "error",
  "css.lint.unknownProperties": "error",
  "css.lint.fontFaceProperties": "error",
  "scss.lint.float": "error",
  "scss.lint.boxModel": "error",
  "scss.lint.important": "error",
  "scss.lint.emptyRules": "error",
  "scss.lint.hexColorLength": "error",
  "scss.lint.idSelector": "error",
  "scss.lint.unknownProperties": "error",
  "scss.lint.fontFaceProperties": "error",
  "workbench.sideBar.location": "left",
  "code-runner.executorMap": {
    "cpp": "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
  },
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "zsh",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "git.untrackedChanges": "separate",
  "gitlens.gitCommands.closeOnFocusOut": true,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "switch:command"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
}
