{
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.matchBrackets": false,
  "editor.wordBasedSuggestions": true,
  "editor.renderLineHighlight": "gutter",
  "editor.tabSize": 2,
  "editor.detectIndentation": true,
  "editor.tabCompletion": "on",
  "editor.fontSize": 16,
  "prettier.singleQuote": true,
  "prettier.tabWidth": 2,
  "prettier.printWidth": 80,
  "prettier.bracketSpacing": true,
  "prettier.semi": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSaveTimeout": 300,
  "workbench.iconTheme": "vscode-icons",
  "formattingToggle.activateFor": [
    "formatOnPaste",
    "formatOnSave",
    "formatOnType"
  ],
  "window.zoomLevel": -1,
  "files.autoSave": "off",
  "files.associations": {
    "*.json": "jsonc"
  },
  "eslint.autoFixOnSave": true,
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[handlebars]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[css]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": true,
  "terminal.integrated.rendererType": "dom",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "typescript.format.enable": false,
  "typescript.suggestionActions.enabled": false,
  "typescript.tsc.autoDetect": "off",
  "typescript.validate.enable": true,
  "javascript.validate.enable": false,
  "breadcrumbs.enabled": true,
  "flow.enabled": false,
  "sync.gist": "ef480108830c863014df47e35067f78c",
  "sync.autoUpload": true,
  "sync.syncExtensions": true,
  "git.autofetch": true,
  "todohighlight.isEnable": false,
  "cSpell.enabled": false,
  "editor.lineNumbers": "on",
  "C_Cpp.updateChannel": "Insiders",
  "sync.forceUpload": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "code-runner.runInTerminal": true,
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, ColumnLimit: 80, PointerAlignment: Left}",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.configuration.checkProjectSettingsExclusions": false,
  "htmlhint.documentSelector": [
    "html",
    "htm",
    "twig",
    "pug",
    "ejs",
    "hbs",
    "js"
  ],
  "htmlhint.options": {
    "tagname-lowercase": false,
    "attr-lowercase": true,
    "attr-value-double-quotes": true,
    "doctype-first": true,
    "src-not-empty": true,
    "title-require": true,
    "id-unique": true,
    "doctype-html5": true,
    "alt-require": true,
    "style-disabled": true,
    "inline-style-disabled": true,
    "inline-script-disabled": true
  },
  "csstree.enable": true,
  "css.lint.float": "error",
  "css.lint.boxModel": "error",
  "css.lint.important": "error",
  "css.lint.emptyRules": "error",
  "css.lint.hexColorLength": "error",
  "css.lint.idSelector": "error",
  "css.lint.unknownProperties": "error",
  "css.lint.fontFaceProperties": "error",
  "scss.lint.float": "error",
  "scss.lint.boxModel": "error",
  "scss.lint.important": "error",
  "scss.lint.emptyRules": "error",
  "scss.lint.hexColorLength": "error",
  "scss.lint.idSelector": "error",
  "scss.lint.unknownProperties": "error",
  "scss.lint.fontFaceProperties": "error",
  "workbench.colorTheme": "Material Theme High Contrast",
  "workbench.sideBar.location": "left",
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.default.cStandard": "c11",
  "code-runner.executorMap": {
    "cpp": "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
  },
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "zsh"
}
